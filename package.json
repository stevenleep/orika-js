{
  "name": "orika-js",
  "version": "1.2.1",
  "private": true,
  "description": "TypeScript object mapping library for PO/DTO/VO transformations",
  "author": "Steven Lee",
  "license": "MIT",
  "homepage": "https://github.com/stevenleep/orika-js#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/stevenleep/orika-js.git"
  },
  "scripts": {
    "build": "pnpm -r --filter './packages/*' run build",
    "build:core": "pnpm --filter '@orika-js/core' run build",
    "build:vue3": "pnpm --filter '@orika-js/vue3' run build",
    "dev": "pnpm -r --parallel --filter './packages/*' run dev",
    "dev:core": "pnpm --filter '@orika-js/core' run dev",
    "dev:vue3": "pnpm --filter '@orika-js/vue3' run dev",
    "example:vue3:app": "cd examples/vue3-real-app && pnpm dev"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@8.15.0"
}
