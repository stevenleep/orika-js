{
  "name": "@orika-js/core",
  "version": "1.2.1",
  "description": "TypeScript object mapping library for PO/DTO/VO transformations",
  "author": "Steven Lee",
  "license": "MIT",
  "homepage": "https://github.com/stevenleep/orika-js/tree/main/packages/core#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/stevenleep/orika-js.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/stevenleep/orika-js/issues"
  },
  "keywords": [
    "mapper",
    "dto",
    "po",
    "vo",
    "object-mapping",
    "typescript",
    "converter",
    "type-safe",
    "mapping",
    "transformation",
    "entity",
    "model"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16"
  }
}

