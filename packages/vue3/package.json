{
  "name": "@orika-js/vue3",
  "version": "1.0.0",
  "description": "Vue 3 adapter for orika-js - Reactive object mapping for Vue 3",
  "author": "Steven Lee",
  "license": "MIT",
  "homepage": "https://github.com/stevenleep/orika-js/tree/main/packages/vue3#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/stevenleep/orika-js.git",
    "directory": "packages/vue3"
  },
  "bugs": {
    "url": "https://github.com/stevenleep/orika-js/issues"
  },
  "keywords": [
    "orika-js",
    "vue3",
    "vue",
    "pinia",
    "mapper",
    "reactive",
    "composition-api",
    "typescript"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@orika-js/core": "^1.2.0",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "pinia": {
      "optional": true
    }
  },
  "devDependencies": {
    "@orika-js/core": "workspace:*",
    "@types/node": "^20.0.0",
    "pinia": "^2.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vue": "^3.4.0"
  },
  "engines": {
    "node": ">=16"
  }
}

